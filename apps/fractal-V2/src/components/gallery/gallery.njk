<cor-gallery   style="display: block">
  <div id="gallery-container" >
  {% for image in gallery %}
      <a data-sub-html="{{ image.description }}" data-src="{{ image.datasrc }}" class="gallery-item">
            <img alt="test  {{ image.alt }}" src="{{ image.src }}" />
      </a>
    {% endfor %}
  </div>
</cor-gallery>




