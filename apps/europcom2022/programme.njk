---
layout: layouts/programme.njk
permalink: /programme/
title: Programme
eleventyNavigation:
  key: Programme
  order: 2
---

<ul class="europcom-stack europcom-program">
  {% for item in program %}
  <li class="europcom-program__item">
    <span class="europcom-program__item__date">{{ item.date }}</span>
    <h3 class="europcom-program__item__title"><a href="{{ '/item' | url }}">{{ item.title }}</a></h3>
    <p>{{ item.description }}</p>
    <ul class="europcom-program__speakerlist">
      {% for speaker in item.speakers %}
      <li class="europcom-program__speaker">
        <div class="europcom-program__avatar">
          <img class="img" src='https://dummyimage.com/100x100.gif' alt='portrait of {{ speaker.name }}'/>
        </div>
        <div>
          <strong>{{ speaker.name }}</strong><br/>
          {{ speaker.title }}
        </div>
      </li>
      {% endfor %}
    </ul>

    <a class="europcom-program__link" href="{{ 'programitem' | url }}">
