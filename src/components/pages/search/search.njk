<script language="javascript">
$(window).ready(function(){	
	var pageName = document.location.pathname.substr(document.location.pathname.lastIndexOf('/') + 1).toLowerCase();
	switch(pageName){
		case "search.aspx": 
			$("#SearchEverything").addClass("active");		
			break;
		case "search_op.aspx":
			$("#SearchOpinions").addClass("active");		
			break;
		case "search_docs.aspx":
			$("#SearchDocuments").addClass("active");
			break;
		case "search_news.aspx":
			$("#SearchNews").addClass("active");
			break;			
		case "search_events.aspx":
			$("#SearchEvents").addClass("active");
			break;			
	}	 
	
	SP.SOD.executeFunc('sp.js', 'SP.ClientContext', getResourceVal);	
	
	//get resource strings from ‘EventNotification’ resource file depending on the current language
    function getResourceVal() {   	    
	    currentUICluture = _spPageContextInfo.currentUICultureName;
        var resourceUrl = '/_layouts/ScriptResx.ashx?culture=' + currentUICluture + '&name=Resource_Website';

        $.getScript(resourceUrl, function () {
            var allContents = Res['search_AllContents'];
            var news = Res['tab_News'];
            var events = Res['tab_Events'];
            var opinions = Res['tab_Opinions'];
            var documents = Res['search_Documents'];  
            var search = Res['hd_SearchText'];                                              
 
	        $("#SearchEverything").text(allContents);
	        $("#SearchNews").text(news);
	        $("#SearchEvents").text(events);
	        $("#SearchOpinions").text(opinions);
	        $("#SearchDocuments").text(documents);
	        $("input[id$='csr_sbox']").val(search + "...");


                     
        });        
    }
});
