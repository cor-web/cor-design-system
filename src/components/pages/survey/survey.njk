<div class="bg-white">

  {% include "@page-header" %}  

  <div class="cor-center">

    <div class="cor-stack">
      {% for question in questions %}
        {% if question.type === "bargraph" %}
          {% render "@bar-graph--withfilter", question %}
        {% elif question.type === "multibargraph" %}
          {% render "@multi-bar-graph--withfilter", question %}
        {% else %}

        {% endif %}
      {% endfor %}
    </div>

  </div>
</div>

