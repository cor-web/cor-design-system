{% for member in members %}
<div class="cor-media">
    <div class="cor-media__img cor-stack">
      <img class="cor-image aspect-square" src="{{ member.picture }}" alt="portrait of {{ member.name }}"/>
    </div>
    
    <strong class="txt-step--2 cor-media__title">{{ member.name }}</strong>
    <div class="cor-media__content cor-stack  cor-stack--3xs">
      
      <address class="txt-step--3 cor-stack cor-stack--4xs">
        <a href="" class="txt-step--3 icon icon-before" style="--icon: var(--icon-phone)">{{ member.phone }}</a>
        <a href="" class="txt-step--3 icon icon-before" style="--icon: var(--icon-envelope)">{{ member.email }}</a>
        <span class="txt-step--3 icon icon-before" style="--icon: var(--icon-geo)">Leuvenseweg 86<br/>
        1000 Bruxelles - Brussel<br/>
        Belgique/BelgiÃ«
        </span>
      </address>
    </div>

</div>
{% endfor %}
