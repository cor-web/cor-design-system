    "vite": "^2.9.13",
    "vite-plugin-nunjucks": "^0.1.10",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "postcss-css-variables": "^0.18.0",
    "rollup": "^3.2.3",
    "deepmerge": "^4.2.2",
    "rimraf": "^3.0.2",
    "@open-wc/building-rollup": "^2.0.2",
    "rollup-plugin-version-injector": "^1.3.3",
    "moment": "^2.29.4"
